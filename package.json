{
  "name": "vinlib-ui",
  "private": true,
  "version": "0.0.1",
  "description": "Vinlib UI",
  "repository": "",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite --https --port 4000",
    "build": "cross-env NODE_ENV=production vite build",
    "postinstall": "cpy ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy ./node_modules/material-icons/iconfont/*.* ./src/fonts/"
  },
  "browserslist": [
    "Android >= 7",
    "IOS >= 11",
    "Safari >= 11",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "apollo-upload-client": "^16.0.0",
    "dom7": "^3.0.0",
    "framework7": "^6.3.4",
    "framework7-icons": "^5.0.3",
    "framework7-react": "^6.3.4",
    "graphql": "^15.6.1",
    "graphql-upload": "^12.0.0",
    "jslib-html5-camera-photo": "^3.1.8",
    "material-icons": "^1.3.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-refresh": "^0.10.0",
    "react-webcam": "^6.0.0",
    "skeleton-elements": "^3.5.0",
    "swiper": "^6.8.4"
  },
  "devDependencies": {
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "cpy-cli": "^1.0.1",
    "cross-env": "^7.0.3",
    "postcss-preset-env": "^6.7.0",
    "vite": "^2.5.10"
  }
}
